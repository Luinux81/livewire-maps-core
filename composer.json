{
    "name": "lbcdev/livewire-maps-core",
    "description": "A Livewire component for interactive maps using Leaflet",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "livewire",
        "laravel",
        "map",
        "leaflet",
        "component"
    ],
    "authors": [
        {
            "name": "LBCDev",
            "email": "luinux81@gmail.com"
        }
    ],
    "require": {
        "php": "^8.1|^8.2|^8.3",
        "illuminate/support": "^10.0|^11.0|^12.0",
        "livewire/livewire": "^3.0",
        "lbcdev/map-geometries": "dev-main"
    },
    "repositories": [
        {
            "type": "path",
            "url": "../geometries",
            "options": {
                "symlink": true
            }
        }
    ],
    "require-dev": {
        "orchestra/testbench": "^10.9",
        "phpunit/phpunit": "^12.5",
        "laravel/pint": "^1.27"
    },
    "autoload": {
        "psr-4": {
            "LBCDev\\LivewireMaps\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "LBCDev\\LivewireMaps\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "LBCDev\\LivewireMaps\\LivewireMapsServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}